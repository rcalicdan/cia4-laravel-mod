{
    "name": "rcalicdan/ci4-larabridge",
    "description": "A CodeIgniter 4 library package with Laravel package integrations and Automatic Setup and more",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rcalicdan\\Ci4Larabridge\\": "src/"
        },
        "files": [
            "src/Helpers/auth_helper.php",
            "src/Helpers/authorization_helper.php",
            "src/Helpers/blade_helper.php",
            "src/Helpers/db_eloquent.php",
            "src/Helpers/http_helper.php",
            "src/Helpers/remember_cache_helper.php",
            "src/Helpers/url_back_helper.php",
            "src/Helpers/url_params.php",
            "src/Helpers/bcrypt_helper.php",
            "src/Helpers/session_redirect_helper.php",
            "src/Helpers/path_helper_constant.php"
        ],
        "exclude-from-classmap": [
            "**/Database/Eloquent-Migrations/**"
        ]
    },
    "authors": [
        {
            "name": "Reymart Calicdan",
            "email": "reymart.calicdan06@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/rcalicdan/ci4-larabridge"
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/collections": "^11.0",
        "illuminate/container": "^11.0",
        "illuminate/contracts": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/events": "^11.0",
        "illuminate/hashing": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/pagination": "^11.0",
        "illuminate/validation": "^11.0",
        "illuminate/view": "^11.0",
        "rcalicdan/blade": "^1.0"
    },
    "require-dev": {
        "codeigniter4/framework": "^4.0",
        "laravel/pint": "^1.10"
    },
    "scripts": {
        "format": "./vendor/bin/pint",
        "format-test": "./vendor/bin/pint --test"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}